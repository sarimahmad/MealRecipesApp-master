{
  "expo": {
    "name": "Jesse Krim - Meals App",
    "slug": "jessekrim-mealsApp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chef-hat-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1dacd6"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.jessekrim.meals-app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jessekrim.meals_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/chef-hat-icon.png",
        "backgroundColor": "#1dacd6"
      }
    },
    "description": "Expo Description: A basic shopping app built to learn React Native"
  }
}
